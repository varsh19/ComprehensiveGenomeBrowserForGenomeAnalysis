{
  "plugins": [
    {
      "name": "Dva",
      "url": "http://localhost:9000/dist/jbrowse-plugin-dva.umd.development.js"
    }
  ],

  "assemblies": [
    {
      "name": "hg38",
      "aliases": ["GRCh38"],
      "sequence": {
        "type": "ReferenceSequenceTrack",
        "trackId": "P6R5xbRqRr",
